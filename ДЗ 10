import string
import keyword

def is_valid_variable_name(variable_name):
    if not variable_name.startswith('_'):
        return False
    
    if variable_name[1:].isdigit() or variable_name[1:] in keyword.kwlist:
        return False
    
    allowed_characters = string.ascii_letters + string.digits + "_"
    if not all(char in allowed_characters for char in variable_name):
        return False
    
    return True

user_input = input("Введіть ім'я змінної: ")

print(is_valid_variable_name(user_input))
